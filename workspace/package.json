{
  "name": "workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:frontend": "ng serve --proxy-config projects/frontend/proxy.conf.json --open",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "quick": "concurrently \"npm run fake-backend\" \"npm run start:frontend\"",
    "fake-backend": "json-server --watch projects/fake-backend/db.json --routes projects/fake-backend/routes.json --port 7627 --middlewares ./node_modules/json-server-reset",
    "format:test": "prettier \"projects/**/*.{ts,html,md,scss,json}\" --list-different",
    "format:write": "prettier \"projects/**/*.{ts,html,md,scss,json}\" --write"
  },
